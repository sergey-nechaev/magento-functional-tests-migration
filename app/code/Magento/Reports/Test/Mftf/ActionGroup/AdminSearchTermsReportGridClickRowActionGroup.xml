<actionGroups xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:mftf:Test/etc/actionGroupSchema.xsd">
    <actionGroup name="AdminSearchTermsReportGridClickRowActionGroup">
        <annotations>
            <description>Click the row on the grid</description>
        </annotations>
        <arguments>
            <argument name="rowNumber" type="string"/>
        </arguments>
        <click selector="{{AdminSearchTermsReportMainSection.gridRow(rowNumber)}}" stepKey="clickRow"/>
    </actionGroup>
</actionGroups>
