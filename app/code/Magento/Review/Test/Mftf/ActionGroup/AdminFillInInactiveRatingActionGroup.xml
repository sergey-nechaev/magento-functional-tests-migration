<actionGroups xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:noNamespaceSchemaLocation="urn:magento:mftf:Test/etc/actionGroupSchema.xsd">
    <actionGroup name="AdminFillInInactiveRatingActionGroup" extends="AdminFillInActiveRatingActionGroup">
        <annotations>
            <description>Fills in the fields on a product rating and sets it to inactive</description>
        </annotations>
        <uncheckOption selector="{{AdminNewRatingSection.isActive}}" stepKey="setIsActive" />
    </actionGroup>
</actionGroups>
