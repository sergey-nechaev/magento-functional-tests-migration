<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">
    <section name="AdminEditRatingSection">
        <element name="ratingCode" type="input" selector="#rating_code"/>
        <element name="stores" type="multiselect" selector="#stores" />
        <element name="isActive" type="checkbox" selector="#is_active" />
        <element name="position" type="input" selector="#position"/>
        <element name="backButton" type="button" selector="#back"/>
        <element name="deleteButton" type="button" selector="#delete"/>
        <element name="resetButton" type="button" selector="#reset"/>
        <element name="saveButton" type="button" selector="#save"/>
        <element name="confirmDelete" type="button" selector=".action-primary.action-accept"/>
    </section>
</sections>
